<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="420.0" prefWidth="740.0" spacing="20.0" style="-fx-background-image: url(&quot;../StartGameButton.png&quot;);" styleClass="Menu" stylesheets="@GuiStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uom.team7.PrimaryController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@risck_logo.png" />
         </image>
      </ImageView>
      <Label text="CHOOSE PLAYERS">
         <font>
            <Font size="20.0" />
         </font></Label>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="50.0">
         <children>
            <ToggleButton mnemonicParsing="false" styleClass="RoundButton" stylesheets="@GuiStyle.css" text="2" />
            <ToggleButton mnemonicParsing="false" styleClass="RoundButton" stylesheets="@GuiStyle.css" text="3" />
            <ToggleButton mnemonicParsing="false" styleClass="RoundButton" stylesheets="@GuiStyle.css" text="4" />
            <ToggleButton mnemonicParsing="false" styleClass="RoundButton" stylesheets="@GuiStyle.css" text="5" />
            <ToggleButton mnemonicParsing="false" styleClass="RoundButton" stylesheets="@GuiStyle.css" text="6" />
         </children>
      </HBox>
      <Button fx:id="primaryButton" accessibleText="Start game" contentDisplay="CENTER" onAction="#switchToSecondary" prefHeight="413.0" prefWidth="910.0" style="-fx-background-image: url(&quot;@StartGameButton.png&quot;);" text="START GAME" textAlignment="CENTER" />
      <ImageView fitHeight="150.0" fitWidth="200.0" onMouseClicked="#switchToSecondary" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@StartGameButton.png" />
         </image></ImageView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
